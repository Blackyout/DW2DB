<Window x:Class="DW2DB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DW2DB"
        mc:Ignorable="d"
        Title="{DynamicResource Title}" Height="350" Width="525" d:DataContext="{d:DesignInstance local:MainWindowVM }">
    <DockPanel>
        <DockPanel.DataContext>
            <local:MainWindowVM/>
        </DockPanel.DataContext>
        <Menu DockPanel.Dock="Top" Background="{x:Null}">
            <MenuItem x:Name="MenuLanguage" Header="{DynamicResource MenuLanguage}" />
        </Menu>
  
      
    </DockPanel>
</Window>
